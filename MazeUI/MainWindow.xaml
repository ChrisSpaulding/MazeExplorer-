        xmlns:local="clr-namespace:MazeUI"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="btnRunMaze" Content="Run Maze" HorizontalAlignment="Left" Margin="88,132,0,0" VerticalAlignment="Top" Width="106" Click="Button_Click" Height="50"/>
        <TextBox x:Name="txtNumberOfMovesMadePrintOut" HorizontalAlignment="Left" Height="170" Margin="570,132,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="107" IsEnabled="False"/>
